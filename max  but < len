input_str = input().split(',')
final = ""

for a in input_str:
    x,y = a.split(':')
    length = len(x);
    max = 0;

    for i in y:
        if int(i)>=int(max) and int(i)<=length:
           max = i
    if max == 0:
        final += 'X'
    else:
        final += x[int(max)-1]
    
print(final)

