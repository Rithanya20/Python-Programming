str_input = input()
even, odd = [], []
count = 0
for i in str_input:
    if not (i.isalnum()):
        count += 1
    if(i.isdigit()):
        if(int(i)%2 == 0):
            even.append(i)
        else:
            odd.append(i)
if count% 2!=0:
    odd, even = even, odd
for b in range(max(len(even), len(odd))):
    if b < len(even):
        print(even[b], end = '')
    if b < len(odd):
        print(odd[b], end = '')
