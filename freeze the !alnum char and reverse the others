str_inp = input()
output = list(str_inp)
#length = len(str_inp)
i=0
j = len(str_inp)-1
while i<j:
    if not output[i].isalnum():
        i += 1
    elif not output[j].isalnum():
        j -= 1
    else:
        output[i],output[j] = output[j], output[i]
        i += 1
        j -= 1
        
print("".join(output))
